      if (currentDate.getFullYear() > 2027 || (currentDate.getFullYear() === 2027 && currentDate.getMonth() > 0)) {
        currentDate = new Date(2027, 0, 1); // Set to January 1, 2027
      }

      generateCalendar();
    }

